{
    "files.autoSave": "onFocusChange",
    "git.autofetch": true,
    "editor.fontFamily": "\"Jetbrains Mono\", Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "explorer.compactFolders": false,
    "git.confirmSync": false,
    "todo-tree.tree.showScanModeButton": false,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "window.autoDetectColorScheme": true,
    "autoDarkMode.lightTheme": "GitHub Light",
    "yaml.schemas": {
        "file:///Users/evgkirov/.vscode/extensions/atlassian.atlascode-2.10.12/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "atlascode.jira.jqlList": [
        {
            "id": "42caf30c-f41c-4a25-bd4c-1bbfa376ea7d",
            "enabled": true,
            "name": "My autofut Issues",
            "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
            "siteId": "51669127-8c9a-4233-bc2c-8a7769243f64",
            "monitor": true
        },
        {
            "id": "e2d5848b-abc9-4465-aae0-af8042f7b2b4",
            "siteId": "51669127-8c9a-4233-bc2c-8a7769243f64",
            "name": "All open issues",
            "query": "resolution = Unresolved ORDER BY lastViewed DESC",
            "enabled": true,
            "monitor": true
        }
    ],
    "python.showStartPage": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.enableSmartCommit": true,
    "redhat.telemetry.enabled": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "terminal.external.osxExec": "iTerm.app",
    "customizeUI.titleBar": "inline",
    "window.titleBarStyle": "native",
    "customizeUI.activityBar": "bottom",
    "customizeUI.statusBarPosition": "under-panel",
    "customizeUI.activityBarHideSettings": true,
    "files.exclude": {
        "**/__pycache__": true
    },
    "workbench.colorTheme": "GitHub Light"
}